<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aplan.mapper.HeatMapper">


    <select id="selectByTypeAndPlatFormTypeAndTitle" resultType="com.aplan.bean.model.HeatModel">
        select * from sys_heat where type = #{type} and platform_type = #{platformType} and title = #{title}
    </select>

</mapper>
